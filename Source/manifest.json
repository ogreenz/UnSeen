{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "FB Chat Privacy",
  "version": "1",
  "manifest_version": 2,
  "description": "Blocks the typing and seen notifications.",
  "background": {
    "scripts": ["blocker.js"]
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ],

  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
